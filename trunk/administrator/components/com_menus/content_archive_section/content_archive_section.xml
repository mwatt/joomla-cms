<?xml version="1.0" encoding="iso-8859-1"?>
<mosparams type="menu" version="4.5.2">
	<name>Blog - Content Section Archive</name>
	<group>Content</group>
	<version>4.5.2</version>
	<description>DescShowsAListingOfContentItemsArchivedForASection</description>
	<params>
		<param name="orderby_pri" type="list" default="" label="Category Order" description="Order items by category">
			<option value="">No, order by Primary Order only</option>
			<option value="alpha">Title Alphabetical</option>
			<option value="ralpha">Title Reverse-Alphabetical</option>
			<option value="order">Ordering</option>
		</param>
		<param name="orderby_sec" type="list" default="" label="Primary Order" description="paramOrderBySec">
			<option value="">Default</option>
			<option value="date">Oldest first</option>
			<option value="rdate">Most recent first</option>
			<option value="alpha">Title Alphabetical</option>
			<option value="ralpha">Title Reverse-Alphabetical</option>
			<option value="author">Author Alphabetical</option>
			<option value="rauthor">Author Reverse-Alphabetical</option>
			<option value="hits">Most Hits</option>
			<option value="rhits">Least Hits</option>
			<option value="order">Ordering</option>
		</param>
		<param name="@spacer" type="spacer" default="" label="" description="" />
 		<param name="description" type="radio" default="0" label="Description" description="Show/Hide the Section Description">
 			<option value="0">Hide</option>
 			<option value="1">Show</option>
 		</param>
		<param name="description_image" type="radio" default="0" label="Description Image" description="Show/Hide the Section Description">
 			<option value="0">Hide</option>
 			<option value="1">Show</option>
 		</param>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="category" type="radio" default="0" label="Category Name" description="paramCateg">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="category_link" type="radio" default="0" label="Category Name Linkable" description="paramCategLink">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
	</params>
</mosparams>
