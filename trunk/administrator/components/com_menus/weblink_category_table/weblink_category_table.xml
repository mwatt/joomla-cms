<?xml version="1.0" encoding="iso-8859-1"?>
<mosparams type="menu" version="4.5.2">
	<name>Table - Weblink Category</name>
	<group>Component</group>
	<description>Shows a Table view of Weblink items for a particular Weblink Category</description>
	<params>
		<param name="headings" type="radio" default="1" label="Table Headings" description="Show/Hide the Table Headings">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="hits" type="list" default="" label="Hits" description="Show/Hide the Hits column">
			<option value="">Use Global</option>
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="item_description" type="radio" default="1" label="Link Descriptions" description="paramItemDesc">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="other_cat" type="radio" default="1" label="Other Categories" description="paramOtherCat">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
	</params>
</mosparams>
